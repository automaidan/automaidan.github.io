{
  "name": "scrapper",
  "version": "0.0.2",
  "description": "Screpper part of judges application.",
  "main": "index.js",
  "dependencies": {
    "bluebird": "3.5.0",
    "csvtojson": "1.1.6",
    "json2csv": "3.7.3",
    "levenshtein-string-distance": "1.2.1",
    "lodash": "4.13.1",
    "moment": "2.18.1",
    "nconf": "0.8.4",
    "node-fetch": "1.5.2",
    "transliteration": "1.0.5"
  },
  "devDependencies": {
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.7.0",
    "tape": "4.6.3"
  },
  "scripts": {
    "test": "tape '**/*.test.js'",
    "create-folders": "mkdir -p ../{profiles,source};",
    "erase": "rimraf ../profiles",
    "scrap": "npm run create-folders; SCRAPPER_SPEED=20 node --max-old-space-size=4096 index.js",
    "scrap:flightcheck": "npm run create-folders; SCRAPPER_SPEED=90 PERSONS_LIMIT=2 node $NODE_DEBUG_OPTION index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/automaidan/juwdges.git"
  },
