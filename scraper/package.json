{
  "name": "scrapper",
  "version": "0.0.2",
  "description": "Screpper part of judges application.",
  "main": "index.js",
  "dependencies": {
    "bluebird": "3.5.0",
    "csvtojson": "1.1.6",
    "json2csv": "3.7.3",
    "levenshtein-string-distance": "1.2.1",
    "lodash": "4.13.1",
    "moment": "2.18.1",
    "nconf": "0.8.4",
    "node-fetch": "1.5.2",
    "transliteration": "1.0.5"
  },
  "devDependencies": {
    "tape": "4.6.3"
  },
  "scripts": {
    "test": "tape 'test/**/*.js'",
    "create-folders": "mkdir -p ../{profiles,source};",
    "erase": "rimraf ../profiles",
    "scrap": "npm run create-folders; SCRAPPER_SPEED=45 node index.js",
    "scrap:flightcheck": "npm run create-folders; SCRAPPER_SPEED=35 PERSONS_LIMIT=2 node index.js"
  },
